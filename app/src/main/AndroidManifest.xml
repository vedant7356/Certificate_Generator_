<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.certificate_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Certificate_app">
        <activity android:name=".Generating_Certificate"></activity>
        <activity android:name=".Registered_Users" />
        <activity android:name=".Admin_Login" />
        <activity android:name=".User_dashboard" />
        <activity android:name=".Admin_Activity" />
        <activity android:name=".Register_Screen" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>